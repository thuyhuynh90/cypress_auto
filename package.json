  "description": "",
  "devDependencies": {
    "cypress": "^13.12.0",
    "cypress-multi-reporters": "^1.6.4",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "start-test": "^0.0.1",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "junit-merge": "^2.0.0"
  }
