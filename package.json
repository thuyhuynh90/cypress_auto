{
  "name": "practice_cypress_automationexercisepage",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "e2e": "ng e2e",
    "delete:reports": "del /S /Q cypress\\results\\*", 
    "prereport": "npm run delete:reports",
    "cypress:run": "npm run prereport && npx cypress run",
    "mochawsome:merge": "npx mochawesome-merge \"cypress/results/mochawesome/*.json\" > mochawesome.json && npx marge mochawesome.json",
    "junit:merge": "npx junit-merge -d cypress/results/junit/ -o cypress/results/junit/result.xml",
    "cypress:e2e": "npm run cypress:run && npm run junit:merge && npm run mochawsome:merge"
  },
